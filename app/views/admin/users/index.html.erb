<h1>Seller Details</h1>
<h3><%= link_to "Home Page", admin_homes_path	%></h3>
<table border="2" style="width:100%;padding:10px;">
  <tr>
    <th>Id</th>
    <th>Name</th>
    <th>Email</th>
    <th>Delete</th>
  </tr>
  <tbody>
    <% @users.each do |user| %>
      <tr style="text-align:center">
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= button_to 'Destroy', { controller: "admin/users", action: :destroy, id: user.id }, method: :delete %></td>
      </tr>
    <%end%>
  </tbody>
</table>